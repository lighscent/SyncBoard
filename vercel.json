{
    "version": 2,
    "builds": [
        {
            "src": "main.js",
            "use": "@vercel/node"
        },
        {
            "src": "views/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.js"
        }
    ]
}